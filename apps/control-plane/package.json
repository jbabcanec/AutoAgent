{
  "name": "@autoagent/control-plane",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "db:backup": "tsx src/scripts/dbAdmin.ts backup",
    "db:restore": "tsx src/scripts/dbAdmin.ts restore",
    "lint": "node -e \"console.log('lint: pending')\"",
    "test": "tsx --test src/__tests__/*.test.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "tsx": "^4.20.5"
  }
}

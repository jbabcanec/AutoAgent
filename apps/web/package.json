{
  "name": "@autoagent/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist-main/main/main.js",
  "scripts": {
    "build": "pnpm run build:main && pnpm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "dev": "concurrently -k \"pnpm run dev:renderer\" \"pnpm run dev:main\" \"pnpm run dev:electron\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 file:dist-main/main/main.js && electron dist-main/main/main.js",
    "start": "electron dist-main/main/main.js",
    "lint": "node -e \"console.log('lint: pending')\"",
    "test": "tsx --test src/main/runLifecycle.test.ts",
    "typecheck": "tsc -p tsconfig.main.json --noEmit && tsc -p tsconfig.renderer.json --noEmit",
    "package": "pnpm run build && electron-builder --dir",
    "dist": "pnpm run build && electron-builder"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "lucide-react": "^0.575.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^5.4.10",
    "wait-on": "^8.0.5"
  },
  "build": {
    "appId": "com.autoagent.desktop",
    "productName": "AutoAgent Desktop",
    "files": [
      "dist-main/**/*",
      "dist-renderer/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    }
  }
}
